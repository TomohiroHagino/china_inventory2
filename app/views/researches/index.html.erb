<p id="notice"><%= notice %></p>

<h1>Researches</h1>

<table>
  <thead>
    <tr>
      <th>Japan image url</th>
      <th>Japan title</th>
      <th>Japan url</th>
      <th>Japan description</th>
      <th>Japan price</th>
      <th>China image url</th>
      <th>China title</th>
      <th>China url</th>
      <th>China price</th>
      <th>Simulate price</th>
      <th>Jpn research check</th>
      <th>Chn research check</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @researches.each do |research| %>
      <tr>
        <td><%= research.japan_image_url %></td>
        <td><%= research.japan_title %></td>
        <td><%= research.japan_url %></td>
        <td><%= research.japan_description %></td>
        <td><%= research.japan_price %></td>
        <td><%= research.china_image_url %></td>
        <td><%= research.china_title %></td>
        <td><%= research.china_url %></td>
        <td><%= research.china_price %></td>
        <td><%= research.simulate_price %></td>
        <td><%= research.jpn_research_check %></td>
        <td><%= research.chn_research_check %></td>
        <td><%= research.team %></td>
        <td><%= link_to 'Show', research %></td>
        <td><%= link_to 'Edit', edit_research_path(research) %></td>
        <td><%= link_to 'Destroy', research, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Research', new_research_path %>
